{
  "name": "errobj",
  "version": "3.0.0",
  "description": "☠️ Serialise errors to literal (JSONable) object",
  "keywords": [
    "error-logger",
    "serialise",
    "error-details",
    "stack",
    "trace",
    "isomorphic",
    "☠️"
  ],
  "author": "omrilotan",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omrilotan/errobj.git"
  },
  "homepage": "https://omrilotan.com/errobj/",
  "type": "module",
  "main": "./index.js",
  "module": "./index.mjs",
  "browser": "./index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "browser": {
        "import": "./index.mjs",
        "require": "./index.js"
      },
      "node": {
        "import": "./index.mjs",
        "require": "./index.js"
      },
      "import": "./index.mjs",
      "require": "./index.js",
      "default": "./index.js"
    }
  },
  "types": "./index.d.ts",
  "scripts": {
    "test": "jest",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "build": "./scripts/build.js",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "error-stack-parser": "^2.1.4"
  },
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "esbuild": "^0.17.15",
    "esbuild-node-externals": "^1.5.0",
    "jest": "^29.3.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3"
  }
}
